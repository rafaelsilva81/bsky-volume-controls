{
  "name": "Bsky Volume Controls",
  "description": "Adds a volume slider to videos on Bluesky (https://bsky.app).",
  "homepage_url": "https://github.com/llagerlof/fresh-chrome-extension",
  "version": "3.1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Fresh Chrome Extension boilerplate 3.1.0",
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "16": "assets/icons/icon16.png",
      "32": "assets/icons/icon32.png",
      "64": "assets/icons/icon64.png",
      "128": "assets/icons/icon128.png"
    }
  },
  "icons": {
    "16": "assets/icons/icon16.png",
    "32": "assets/icons/icon32.png",
    "64": "assets/icons/icon64.png",
    "128": "assets/icons/icon128.png"
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "js": ["src/content/before.js"],
      "matches": ["https://*/*", "http://*/*"]
    },
    {
      "run_at": "document_idle",
      "js": ["src/content/after.js"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ],
  "permissions": ["tabs", "scripting"],
  "host_permissions": ["https://*/*", "http://*/*"]
}
